<div class="page container" id="<%= @page_title.downcase %>">
	<article>
		<header class="page-header">
			<h1 class="page-title"><%= @page_title %></h1>
		</header>
		<div class="row">
			<section class="media panel panel-default col-md-6">
				<div class="media-left media-top">
					<a href="http://www.uis.edu/">
						<%= image_tag('https://s3.amazonaws.com/camronkhan/images/uis-logo.png', class: 'media-object logo', alt: 'University of Illinois - Springfield') %>
					</a>
				</div>
				<div class="media-body">
					<h2 class="media-heading">Computer Science <small>B.S.</small></h2>
					<h3 class="h4">University of Illinois <small>Springfield, IL</small></h3>
					<p>Degree Awarded: 2016</p>
					<p>Program GPA: 4.0</p>
					<p>Specialization in Software Engineering</p>
				</div>
			</section>
			<section class="media panel panel-default col-md-6">
				<div class="media-left media-top">
					<a href="http://www.nl.edu/">
						<%= image_tag('https://s3.amazonaws.com/camronkhan/images/nlu-logo.png', class: 'logo', alt: 'National Louis University') %>
					</a>
				</div>
				<div class="media-body">
					<h2 class="media-heading">Secondary Education <small>M.A.</small></h2>
					<h3 class="h4">National Louis University <small>Chicago, IL</small></h3>
					<p>Degree Awarded: 2014</p>
					<p>Program GPA: 4.0</p>
					<p>ISBE Professional Educator License, Grades 6-12</p>
				</div>
			</section>
			<section class="media panel panel-default col-md-6">
				<div class="media-left media-top">
					<a href="http://www.luc.edu/">
						<%= image_tag('https://s3.amazonaws.com/camronkhan/images/luc-logo.png', class: 'logo', alt: 'Loyola University - Chicago') %>
					</a>
				</div>
				<div class="media-body">
					<h2 class="media-heading">Psychology <small>B.S.</small></h2>
					<h3 class="h4">Loyola University <small>Chicago, IL</small></h3>
					<p>Degree Awarded: 2008</p>
					<p>Program GPA: 3.7</p>
					<p>Cum Laude Graduate with Honors in Psychology</p>
				</div>
			</section>
			<section class="media panel panel-default col-md-6">
				<div class="media-left media-top">
					<a href="http://www.luc.edu/">
						<%= image_tag('https://s3.amazonaws.com/camronkhan/images/luc-logo.png', class: 'logo', alt: 'Loyola University - Chicago') %>
					</a>
				</div>
				<div class="media-body">
					<h2 class="media-heading">Philosophy <small>B.A.</small></h2>
					<h3 class="h4">Loyola University <small>Chicago, IL</small></h3>
					<p>Degree Awarded: 2008</p>
					<p>Program GPA: 3.9</p>
					<p>Cum Laude Graduate with Honors in Philosophy</p>
				</div>
			</section>
		</div>
	</article>
</div>